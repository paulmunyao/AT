<!doctype html>
<html>

<head>
    <title>Blanqx Cloud Dialer</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="https://fonts.googleapis.com/css?family=Exo" rel="stylesheet">
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css" rel="stylesheet">
    <script src="https://code.jquery.com/jquery-2.2.4.min.js" type="text/javascript"></script>
    <script src="https://www.cssscript.com/demo/animated-gradient-background-javascript/build/gradientify.js"></script>
    <link href="css/keypad.css" rel="stylesheet">
</head>

<body>
    <div id="status"></div>
    <div class="container">
        <div id="output"></div>
        <div class="keypadHolder">
            <div class="row">
                <div class="digit" id="one">1</div>
                <div class="digit" id="two">
                    2
                    <div class="sub">ABC</div>
                </div>
                <div class="digit" id="three">
                    3
                    <div class="sub">DEF</div>
                </div>
            </div>
            <div class="row">
                <div class="digit" id="four">
                    4
                    <div class="sub">GHI</div>
                </div>
                <div class="digit" id="five">
                    5
                    <div class="sub">JKL</div>
                </div>
                <div class="digit">
                    6
                    <div class="sub">MNO</div>
                </div>
            </div>
            <div class="row">
                <div class="digit">
                    7
                    <div class="sub">PQRS</div>
                </div>
                <div class="digit">
                    8
                    <div class="sub">TUV</div>
                </div>
                <div class="digit">
                    9
                    <div class="sub">WXYZ</div>
                </div>
            </div>
            <div class="row">
                <div class="digit">
                </div>
                <div class="digit">0
                </div>
                <div class="digit">
                </div>
            </div>
        </div>
        <div class="botrow">
            <div class="digit">+</div>
            <div id="call"><i class="fa fa-phone" aria-hidden="true"></i></div>
            <i class="fa fa-long-arrow-left dig" aria-hidden="true"></i>
        </div>

    </div>
    <!-- <script src="https://unpkg.com/africastalking-client@1.0.6/build/africastalking.js"></script> -->
    <script src="https://unpkg.com/africastalking-client@1.0.2/build/africastalking.js"></script>
    <!-- <script src="js/africastalking.js" type="text/javascript"></script> -->
    <script src="js/keypad.js" type="text/javascript"></script>
</body>

</html>